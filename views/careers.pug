  body
    #myModal.modal.inmodal(tabindex='-1', role='dialog', aria-hidden='true')
      .modal-dialog 
        .modal-content.animated.bounceInRight
          form#careerForm.m-t(name='careerForm', role='form', action='/career', method='post', enctype='multipart/form-data')
            #application_holder.application-form.topspace-double
              .row
                h3.application-form-title.bottomspace 
                  | Application
                  | Form
                .form-group
                  input#first_name.form-control(type='text', required="required", name='first_name', placeholder='First Name')
                .form-group
                  input#last_name.form-control(type='text', name='last_name', placeholder='Last Name')
                .form-group
                  input#email_address.form-control(type='text', name='email_address', required="required", placeholder='Email')
                .form-group
                  input#mobile.form-control.num(type='text', name='mobile', data-length-value='10',  required="required", placeholder='Mobile Number')
                .form-group
                  input#exp.form-control.num(type='number', name='exp', placeholder='Total Relevant Experience (years)')
                .form-group
                  .fileinput.fileinput-new.input-group(data-provides='fileinput')
                    .form-control(data-trigger='fileinput')
                      i.glyphicon.glyphicon-file.fileinput-exists
                      span.fileinput-filename
                    span.input-group-addon.btn.btn-default.btn-file
                      span.fileinput-new Upload CV
                      span.fileinput-exists Change
                      input#resume.mandate(type='file', name='resume', accept='.doc, .docx,.ppt, .pptx,.txt,.pdf', data-valid-type='nEmpty', data-error-text='Please select file in .pdf, .doc, .docx', placeholder='Select CV/Resume')
                    a.input-group-addon.btn.btn-default.fileinput-exists(href='#', data-dismiss='fileinput') Remove
            .modal-footer
              button.btn.btn-white.close-form(type='button', data-dismiss='modal') Close
              button.btn.btn-primary(type='submit') Send

